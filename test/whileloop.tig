let
	var a := 0
	var b := 2

    function assert_int(actual: int, expected: int) = 
		assert_equal_int("whileloop.tig", actual, expected)
in
	while a < 5 do
	(
		printInt(a);
		if a = 3 then break;
		b := b + 1;
		a := a + 1
	);
	assert_int(a, 3);
    assert_int(b, 5);
    print("whileloop.tig: Passed!")
end
