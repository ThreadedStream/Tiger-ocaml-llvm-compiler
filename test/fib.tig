let
  function fib (n: int): int =
    if n = 0 then 0
    else if n = 1 then 1
    else fib(n-1) + fib(n-2)

  var fib0 := fib(0)
  var fib1 := fib(1)
  var fib5 := fib(5)
  var fib14 := fib(14)
  var fib30 := fib(30)
in
  assert_int(fib0, 0);
  assert_int(fib1, 1);
  assert_int(fib5, 5);
  assert_int(fib14, 377);
  assert_int(fib30, 832040)
end
